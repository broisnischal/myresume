    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-github": "^1.6.0",
    "remix-flat-routes": "^0.6.4",
    "remix-themes": "^1.2.2",
    "sonner": "^1.3.1",
    "spin-delay": "^1.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
