{
  "name": "myresume",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix dev --manual",
    "build": "remix build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "seed": "npx prisma db seed",
    "migrate": "npx prisma migrate dev",
    "push": "npx prisma db push",
    "studio": "dotenv -c db.env -- npx prisma studio",
    "force-migrate": "npx prisma db push --force-reset && npx prisma migrate dev --name init",
    "mig": "npx prisma migrate deploy"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
